# baseURI: http://datashapes.org/sh/tests/shapedefs/scopedef-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/shapedefs/scopedef-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/shapedefs/scopedef-001.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of scopedef-001" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate sh:predicate ;
      sh:focusNode ex:theScope ;
      sh:severity sh:Violation ;
      sh:subject ex:theScope ;
    ] ;
.
ex:Shape
  rdf:type sh:Shape ;
  rdfs:label "Shape" ;
  sh:scope ex:theScope ;
.
ex:theScope
  rdf:type sh:PropertyScope ;
  rdfs:comment "This is invalid because it has no predicate." ;
.
