# baseURI: http://datashapes.org/sh/tests/sparql/derived/derivedValues-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/sparql/derived/derivedValues-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/sparql/derived/derivedValues-001.test>
  rdf:type owl:Ontology ;
  owl:imports <http://datashapes.org/dash> ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidRectangle1 ;
      sh:object 42 ;
      sh:predicate ex:area ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidRectangle1 ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidRectangle2 ;
      sh:object 41 ;
      sh:predicate ex:area ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidRectangle2 ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidRectangle2 ;
      sh:object 42 ;
      sh:predicate ex:area ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidRectangle2 ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidRectangle3 ;
      sh:object 43 ;
      sh:predicate ex:area ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidRectangle3 ;
    ] ;
.
ex:InvalidRectangle1
  rdf:type ex:Rectangle ;
  ex:height 7 ;
  ex:width 6 ;
.
ex:InvalidRectangle2
  rdf:type ex:Rectangle ;
  ex:area 41 ;
  ex:height 7 ;
  ex:width 6 ;
.
ex:InvalidRectangle3
  rdf:type ex:Rectangle ;
  ex:area 42 ;
  ex:area 43 ;
  ex:height 7 ;
  ex:width 6 ;
.
ex:Rectangle
  rdf:type rdfs:Class, sh:Shape ;
  sh:property [
      sh:datatype xsd:integer ;
      sh:derivedValues [
      	a sh:SPARQLValuesDeriver ;
          sh:sparql """
				$this ex:width ?width .
				$this ex:height ?height .
				BIND (?width * ?height AS ?value) .
				""" ;
        ] ;
      sh:predicate ex:area ;
    ] ;
  sh:property [
      sh:datatype xsd:integer ;
      sh:predicate ex:height ;
    ] ;
  sh:property [
      sh:datatype xsd:integer ;
      sh:predicate ex:width ;
    ] ;
.
ex:ValidRectangle1
  rdf:type ex:Rectangle ;
  ex:area 42 ;
  ex:height 7 ;
  ex:width 6 ;
.
