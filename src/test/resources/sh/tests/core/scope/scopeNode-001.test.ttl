# baseURI: http://datashapes.org/sh/tests/core/scope/scopeNode-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/core/scope/scopeNode-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/core/scope/scopeNode-001.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of scopeNode-001" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidResource1 ;
      sh:predicate rdfs:label ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidResource1 ;
    ] ;
.
ex:InvalidResource1
  rdf:type rdfs:Resource ;
  rdfs:label "Invalid resource 1"^^xsd:string ;
.
ex:TestShape
  rdf:type sh:Shape ;
  rdfs:label "Test shape"^^xsd:string ;
  sh:property [
      rdfs:label "label"^^xsd:string ;
      sh:datatype xsd:string ;
      sh:maxCount 0 ;
      sh:predicate rdfs:label ;
    ] ;
  sh:scopeNode ex:InvalidResource1 ;
  sh:scopeNode ex:ValidResource1 ;
.
ex:ValidResource1
  rdf:type rdfs:Resource ;
.
