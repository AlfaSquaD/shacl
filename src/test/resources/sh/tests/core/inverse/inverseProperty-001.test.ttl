# baseURI: http://datashapes.org/sh/tests/core/inverse/inverseProperty-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/core/inverse/inverseProperty-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/core/inverse/inverseProperty-001.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of inverseProperty-001" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
ex:Child
  rdf:type ex:Person ;
.
ex:Father
  rdf:type ex:Person ;
  ex:child ex:Child ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate ex:child ;
      sh:focusNode ex:Father ;
      sh:object ex:Father ;
      sh:severity sh:Violation ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate ex:child ;
      sh:focusNode ex:Mother ;
      sh:object ex:Mother ;
      sh:severity sh:Violation ;
    ] ;
.
ex:Mother
  rdf:type ex:Person ;
  ex:child ex:Child ;
.
ex:Person
  rdf:type rdfs:Class ;
  rdf:type sh:Shape ;
  rdfs:subClassOf rdfs:Resource ;
  sh:inverseProperty [
      sh:predicate ex:child ;
      rdfs:label "parent" ;
      sh:class ex:Person ;
      sh:maxCount 2 ;
      sh:minCount 2 ;
    ] ;
  sh:property [
      sh:predicate ex:child ;
      rdfs:label "child" ;
      sh:class ex:Person ;
    ] ;
.
