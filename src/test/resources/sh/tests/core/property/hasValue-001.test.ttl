# baseURI: http://datashapes.org/sh/tests/core/property/hasValue-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/core/property/hasValue-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/core/property/hasValue-001.test>
  rdf:type owl:Ontology ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
  owl:imports <http://datashapes.org/dash> ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidMalePerson ;
      sh:predicate ex:gender ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidMalePerson ;
    ] ;
.
ex:InvalidMalePerson
  rdf:type ex:MalePerson ;
  ex:gender "female"^^xsd:string ;
  rdfs:label "Invalid male person"^^xsd:string ;
.
ex:MalePerson
  rdf:type rdfs:Class ;
  rdfs:label "Male person"^^xsd:string ;
  rdfs:subClassOf rdfs:Resource ;
.
ex:PersonShape
  rdf:type sh:Shape ;
  rdfs:label "Person shape"^^xsd:string ;
  sh:property [
      rdfs:label "gender"^^xsd:string ;
      sh:datatype xsd:string ;
      sh:hasValue "male"^^xsd:string ;
      sh:predicate ex:gender ;
    ] ;
  sh:scopeClass ex:MalePerson ;
.
ex:ValidMalePerson1
  rdf:type ex:MalePerson ;
  ex:gender "male"^^xsd:string ;
.
ex:ValidMalePerson2
  rdf:type ex:MalePerson ;
  ex:gender "female"^^xsd:string ;
  ex:gender "male"^^xsd:string ;
.
