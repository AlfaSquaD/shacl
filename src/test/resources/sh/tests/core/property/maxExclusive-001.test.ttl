# baseURI: http://datashapes.org/sh/tests/core/property/maxExclusive-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/core/property/maxExclusive-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/core/property/maxExclusive-001.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of maxExclusive-001" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate ex:property ;
      sh:focusNode ex:InvalidResource1 ;
      sh:object 1 ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidResource1 ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate ex:property ;
      sh:focusNode ex:InvalidResource2 ;
      sh:object 2 ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidResource2 ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate ex:property ;
      sh:focusNode ex:InvalidResource3 ;
      sh:object "a" ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidResource3 ;
    ] ;
.
ex:InvalidResource1
  ex:property 1 ;
.
ex:InvalidResource2
  ex:property 2 ;
.
ex:InvalidResource3
  ex:property "a" ;
.
ex:MyShape
  rdf:type sh:Shape ;
  sh:property [
      sh:predicate ex:property ;
      sh:maxExclusive 1 ;
    ] ;
  sh:scopeNode ex:InvalidResource1 ;
  sh:scopeNode ex:InvalidResource2 ;
  sh:scopeNode ex:InvalidResource3 ;
  sh:scopeNode ex:ValidResource1 ;
  sh:scopeNode ex:ValidResource2 ;
.
ex:ValidResource1
  ex:property 0 ;
.
ex:ValidResource2
  ex:property -1 ;
.
