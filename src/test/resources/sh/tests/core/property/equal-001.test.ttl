# baseURI: http://datashapes.org/sh/tests/core/property/equal-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/core/property/equal-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/core/property/equal-001.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of equal-001" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidResource1 ;
      sh:path ex:property1 ;
      sh:severity sh:Violation ;
      sh:sourceConstraintComponent sh:EqualsConstraintComponent ;
      sh:value "A" ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidResource1 ;
      sh:path ex:property1 ;
      sh:severity sh:Violation ;
      sh:sourceConstraintComponent sh:EqualsConstraintComponent ;
      sh:value "B" ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidResource2 ;
      sh:path ex:property1 ;
      sh:severity sh:Violation ;
      sh:sourceConstraintComponent sh:EqualsConstraintComponent ;
      sh:value "A" ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidResource3 ;
      sh:path ex:property1 ;
      sh:severity sh:Violation ;
      sh:sourceConstraintComponent sh:EqualsConstraintComponent ;
      sh:value "A" ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidResource4 ;
      sh:path ex:property1 ;
      sh:severity sh:Violation ;
      sh:sourceConstraintComponent sh:EqualsConstraintComponent ;
      sh:value "B" ;
    ] ;
.
ex:InvalidResource1
  ex:property1 "A" ;
  ex:property2 "B" ;
.
ex:InvalidResource2
  ex:property1 "A" ;
.
ex:InvalidResource3
  ex:property2 "A" ;
.
ex:InvalidResource4
  ex:property1 "A" ;
  ex:property1 "B" ;
  ex:property2 "A" ;
.
ex:MyShape
  rdf:type sh:Shape ;
  sh:property [
      sh:predicate ex:property1 ;
      sh:equals ex:property2 ;
    ] ;
  sh:scopeNode ex:InvalidResource1 ;
  sh:scopeNode ex:InvalidResource2 ;
  sh:scopeNode ex:InvalidResource3 ;
  sh:scopeNode ex:InvalidResource4 ;
  sh:scopeNode ex:ValidResource1 ;
  sh:scopeNode ex:ValidResource2 ;
.
ex:ValidResource1
  ex:property1 "A" ;
  ex:property2 "A" ;
.
ex:ValidResource2
  ex:property1 "A" ;
  ex:property1 "B" ;
  ex:property2 "A" ;
  ex:property2 "B" ;
.
ex:property1
  rdf:type rdf:Property ;
.
ex:property2
  rdf:type rdf:Property ;
.
