# baseURI: http://datashapes.org/sh/tests/core/property/directType-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/core/property/directType-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/core/property/directType-001.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of directType-001" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate ex:value ;
      sh:focusNode ex:InvalidInstance1 ;
      sh:object ex:SubInstance ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidInstance1 ;
    ] ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate ex:value ;
      sh:focusNode ex:InvalidInstance2 ;
      sh:object ex:InvalidInstance2 ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidInstance2 ;
    ] ;
.
ex:InvalidInstance1
  ex:value ex:SubInstance ;
.
ex:InvalidInstance2
  ex:value ex:InvalidInstance2 ;
.
ex:MyShape
  rdf:type sh:Shape ;
  sh:property [
      sh:predicate ex:value ;
      sh:directType ex:SuperClass ;
    ] ;
  sh:scopeNode ex:InvalidInstance1 ;
  sh:scopeNode ex:InvalidInstance2 ;
  sh:scopeNode ex:ValidInstance1 ;
.
ex:SubClass
  rdf:type rdfs:Class ;
  rdfs:subClassOf ex:SuperClass ;
.
ex:SubInstance
  rdf:type ex:SubClass ;
.
ex:SuperClass
  rdf:type rdfs:Class ;
  rdfs:subClassOf rdfs:Resource ;
.
ex:SuperInstance
  rdf:type ex:SuperClass ;
.
ex:ValidInstance1
  ex:value ex:SuperInstance ;
.
