<http://datashapes.org/sh/tests/core/property/classIn-001.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of classIn-001" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
ex:Employee
  rdf:type rdfs:Class ;
  rdf:type sh:Shape ;
  rdfs:label "Employee" ;
  rdfs:subClassOf rdfs:Resource ;
.
ex:Employee_1
  rdf:type ex:Employee ;
  rdfs:label "Employee 1" ;
.
ex:FemalePerson
  rdf:type rdfs:Class ;
  rdf:type sh:Shape ;
  rdfs:label "Female person" ;
  rdfs:subClassOf ex:Person ;
.
ex:FemalePerson_1
  rdf:type ex:FemalePerson ;
  rdfs:label "Female person 1" ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:focusNode ex:InvalidNode1 ;
      sh:path ex:property ;
      sh:severity sh:Violation ;
      sh:sourceConstraintComponent sh:ClassInConstraintComponent ;
      sh:value rdfs:Resource ;
    ] ;
.
ex:InvalidNode1
  rdf:type rdfs:Resource ;
  ex:property rdfs:Resource ;
  rdfs:label "Invalid node1" ;
.
ex:MalePerson
  rdf:type rdfs:Class ;
  rdf:type sh:Shape ;
  rdfs:label "Male person" ;
  rdfs:subClassOf ex:Person ;
.
ex:Person
  rdf:type rdfs:Class ;
  rdf:type sh:Shape ;
  rdfs:label "Person" ;
  rdfs:subClassOf rdfs:Resource ;
.
ex:TestShape
  rdf:type sh:Shape ;
  rdfs:label "Test shape" ;
  sh:property [
      sh:predicate ex:property ;
      sh:classIn (
          ex:Person
          ex:Employee
        ) ;
      sh:name "property" ;
    ] ;
  sh:scopeNode ex:InvalidNode1 ;
  sh:scopeNode ex:ValidNode1 ;
.
ex:ValidNode1
  rdf:type rdfs:Resource ;
  ex:property ex:Employee_1 ;
  ex:property ex:FemalePerson_1 ;
  rdfs:label "Valid node1" ;
.
