# baseURI: http://datashapes.org/sh/tests/core/property/datatype-003.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/core/property/datatype-003.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/core/property/datatype-003.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of datatype-003" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
ex:GraphValidationTestCase
  rdf:type dash:GraphValidationTestCase ;
  dash:expectedResult [
      rdf:type sh:ValidationResult ;
      sh:predicate ex:value ;
      sh:focusNode ex:InvalidInstance1 ;
      sh:object 42 ;
      sh:severity sh:Violation ;
      sh:subject ex:InvalidInstance1 ;
    ] ;
.
ex:InvalidInstance1
  ex:value 42 ;
.
ex:MyShape
  rdf:type sh:Shape ;
  sh:property [
      sh:predicate ex:value ;
      sh:datatypeIn (
          xsd:string
          rdf:langString
        ) ;
    ] ;
  sh:scopeNode ex:InvalidInstance1 ;
  sh:scopeNode ex:ValidInstance1 ;
  sh:scopeNode ex:ValidInstance2 ;
  sh:scopeNode ex:ValidInstance3 ;
.
ex:ValidInstance1
  ex:value "A" ;
.
ex:ValidInstance2
  ex:value "A" ;
.
ex:ValidInstance3
  ex:value "A"@en ;
.
