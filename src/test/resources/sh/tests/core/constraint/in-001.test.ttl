# baseURI: http://datashapes.org/sh/tests/core/constraint/in-001.test
# imports: http://datashapes.org/dash
# prefix: ex

@prefix dash: <http://datashapes.org/dash#> .
@prefix ex: <http://datashapes.org/sh/tests/core/constraint/in-001.test#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://datashapes.org/sh/tests/core/constraint/in-001.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test of in-001" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Created with TopBraid Composer"^^xsd:string ;
.
ex:Green
  rdf:type ex:TestShape ;
  rdfs:label "Green"^^xsd:string ;
.
ex:InvalidInstance
  rdf:type ex:TestShape ;
  rdfs:label "Invalid instance"^^xsd:string ;
.
ex:Red
  rdf:type ex:TestShape ;
  rdfs:label "Red"^^xsd:string ;
.
ex:TestShape
  rdf:type rdfs:Class, sh:Shape ;
  rdfs:label "Test shape"^^xsd:string ;
  sh:constraint [
      rdf:type sh:NodeConstraint ;
      sh:in (
          ex:Green
          ex:Red
          ex:Yellow
        ) ;
    ] ;
.
ex:Yellow
  rdf:type ex:TestShape ;
  rdfs:label "Yellow"^^xsd:string ;
.
